{
  "name": "pet-music-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./config/git/commitizen.js"
    }
  },
  "lint-staged": {
    "*.{js, ts, jsx, tsx}": "eslint --cache --fix",
    "*.{js, ts, jsx, tsx, css, md}": "prettier --write"
  },
  "dependencies": {
    "autoprefixer": "10.4.15",
    "next": "13.4.13",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "cz-customizable": "7.0.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.13",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.0",
    "typescript": "5.1.6"
  }
}
